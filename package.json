{
  "name": "@demo/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "concurrently --kill-others \"npm run start:esbuild\" \"npm run start:webpack\"",
    "start:incremental": "nx run demo:serve-static --port 4202",
    "start:esbuild": "nx run demo:serve:development --open --port 4201",
    "start:webpack": "nx run demo:serve:development-webpack --open --port 4200",
    "start:storybook": "nx run demo:storybook",
    "start:prod": "nx run demo:build:production --parallel && node scripts/express-server.js",
    "show-affected": "nx show projects --affected",
    "show-affected:untracked": "nx show projects --affected --untracked=true",
    "show-affected:uncommitted": "nx show projects --affected --uncommitted=true",
    "lint:app:affected": "nx affected --target=lint --exclude='*,!tag:app'",
    "format": "nx format:write",
    "cy:open": "nx run demo-e2e:e2e -- --skipServe=true --watch",
    "test:ct": "nx run demo:component-test -- --watch=true",
    "test:e2e": "nx run demo-e2e:e2e -- --watch=true --port=5200",
    "compile": "tsc --build apps/demo-e2e"
  },
  "private": true,
